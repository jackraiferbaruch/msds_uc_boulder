    "def max_pairwise_product(numbers):\n",
    "\n",
    "    return sorted(numbers)[-1] * sorted(numbers)[-2]\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    _ = int(input())\n",
    "    input_numbers = list(map(int, input().split()))\n",
    "    print(max_pairwise_product(input_numbers))"
   ]
